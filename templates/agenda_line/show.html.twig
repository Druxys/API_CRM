    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agenda_line.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ agenda_line.date ? agenda_line.date|date('Y-m-d H:i:s') : '' }}</td>
