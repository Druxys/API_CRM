                <th>CreatedAt</th>
                <td>{{ agenda_line.createdAt ? agenda_line.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ agenda_line.updatedAt ? agenda_line.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('agenda_line_index') }}">back to list</a>

